{
  "name": "otp-observations-tool",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:e2e": "ng serve --prod --configuration=e2e",
    "dev": "ng serve --configuration=local",
    "build": "ng build",
    "lint": "ng lint",
    "prod-build": "ng build --output-path=build/ --base-href=/observations-tool/ --aot --prod",
    "staging-build": "ng build --output-path=build/ --base-href=/observations-tool/ --aot --configuration=staging",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "transifex:push": "env $(grep ^TX_TOKEN -m 1 .env | xargs) tx push -s",
    "transifex:pull": "env $(grep ^TX_TOKEN -m 1 .env | xargs) tx pull -a -f && node transifex/format-locales.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@asymmetrik/ngx-leaflet": "^6",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "@sentry/browser": "^7",
    "@sentry/integrations": "^7",
    "@types/leaflet": "~1.9.0",
    "a11y-dialog": "^4.0.1",
    "angular-2-dropdown-multiselect": "~1.5.4",
    "angular2-jsonapi": "^8",
    "angular2-spinner": "^1.0.10",
    "core-js": "^2.4.1",
    "date-fns": "1",
    "exif-js": "^2.3.0",
    "express": "^4.15.2",
    "flatpickr": "~4.6.13",
    "foundation-sites": "~6.6.3",
    "leaflet": "~1.9.0",
    "lodash": "^4.17.21",
    "proj4": "^2.6.1",
    "rxjs": "^6.6.7",
    "rxjs-compat": "6",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular-devkit/core": "^8.0.3",
    "@angular/cli": "8.3.29",
    "@angular/compiler-cli": "^8.2.14",
    "@sentry/cli": "^2.21.5",
    "@types/node": "~6.0.60",
    "codelyzer": "^5.0.1",
    "dotenv": "^4.0.0",
    "node-fetch": "^1.7.3",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^2.8.1"
  }
}
