{
  "name": "otp-observations-tool",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:e2e": "ng serve --prod --env=e2e",
    "dev": "ng serve --env=local",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "prod-build": "ng build --output-path=dist/ --base-href=/observations-tool/ --aot -prod",
    "dev-build": "ng build --output-path=dist/ --base-href=/observations-tool/ --aot",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "transifex:push": "env $(grep ^TX_TOKEN -m 1 .env | xargs) tx push -s",
    "transifex:pull": "env $(grep ^TX_TOKEN -m 1 .env | xargs) tx pull -a && node transifex/format-locales.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@asymmetrik/angular2-leaflet": "^2.1.4",
    "@asymmetrik/ngx-leaflet": "^2.4.0",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "@types/leaflet": "^1.2.0",
    "a11y-dialog": "^4.0.1",
    "angular-2-dropdown-multiselect": "~1.5.4",
    "angular2-jsonapi": "^4.0.3",
    "angular2-spinner": "^1.0.10",
    "angular2-web-worker": "^0.0.6",
    "core-js": "^2.4.1",
    "date-fns": "1",
    "exif-js": "^2.3.0",
    "express": "^4.15.2",
    "flatpickr": "~4.2.3",
    "foundation-sites": "~6.6.3",
    "leaflet": "^1.2.0",
    "lodash": "^4.17.21",
    "node-sass": "6",
    "proj4": "^2.6.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular-devkit/core": "^8.0.3",
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "^4.4.6",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "dotenv": "^4.0.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "node-fetch": "^1.7.3",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "^2.4.0",
    "webpack-bundle-analyzer": "^2.8.1"
  }
}
